<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="@dimen/src_dp_3"
        android:bottom="@dimen/src_dp_3"
        android:left="@dimen/src_dp_3"
        android:right="@dimen/src_dp_3">

        <shape
            android:shape="rectangle">
            <!-- 圆角的半径 -->
            <corners android:radius="@dimen/src_dp_50"/>

            <!-- 填充颜色 -->
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle">
            <corners android:radius="@dimen/src_dp_50"/>

            <solid android:color="@color/transparent"/>

            <stroke
                android:width="@dimen/src_dp_1"
                android:color="@color/white"/>
        </shape>
    </item>
</layer-list>